<h2><%= gettext("Gemiddelde eerste reactietijd") %></h2>

<table class="table">
  <thead class="thead-light">
    <tr>
      <th scope="col"><%= gettext("Klant") %></th>
      <th scope="col"><%= gettext("Gemiddelde") %></th>
    </tr>
  </thead>
  <tbody>
    <%= for group <- @model do %>
      <tr>
        <th scope="row"><%= group.company_name %></th>
        <td><%= group.average_first_response %></td>
      </tr>
    <% end %>
  </tbody>
</table>
